            "environment": [], // 需要添加的环境变量
            "externalConsole": false, // 使用启用外部的终端，如果为false会使用集成终端
            "MIMode": "lldb",
            "preLaunchTask": "build" // 在运行之前，需要指定的任务，一般用来进行build，需要对应的是tasks.json中的label
